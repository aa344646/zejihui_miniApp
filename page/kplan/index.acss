  display: block;
  margin: 0 auto;
  margin-top: 40rpx;
}
.pop-content .title {
  padding: 24rpx;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ipt {
  width: 400rpx;
}
.am-popup-mask {
  z-index: 12;
}
.am-popup-content {
  z-index: 13;
  background: #fff;
  border-radius: 12rpx 12rpx 0 0;
}
.choosePhone {
  padding: 20rpx 20rpx 40rpx 20rpx;
  display: flex;
  flex-wrap: wrap;
}
.choosePhone .phone {
  margin: 15rpx;
  font-size: 27rpx;
  width: 170rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding: 4rpx 16rpx;
  border: 1px solid #999;
  border-radius: 6rpx;
}
.choosePhone .phone.active {
  border: 1px solid #1187f7;
  color: #1187f7;
}
.phoneTitle {
  padding: 24rpx;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
